#include<stdio.h>
#include<math.h>
int main ()
{
    int c = 0;
    int j = 2;
    for(int i = 1; i <= 100; i++)
    {
        for(j = 2; j <= i; j++)
        {
            if(i % j ==0)
                break;
        }
         if(i == j)
            {
                printf("%6d", i);
                c++;
             if(c % 10 == 0)
            printf("\n");
            }
    }
    printf("\n");
    return 0; 
}
